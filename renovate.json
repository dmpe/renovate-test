{
  "enabledManagers": [
    "helm-values"
  ],
  "helm-values": {
    "fileMatch": [
      "control_values.yaml.gotmpl"
    ]
  },
  "regexManagers": [
    {
      "fileMatch": [
        "control_values.yaml.gotmpl"
      ],
      "matchStringsStrategy": "any",
      "matchStrings": [
        "(?<depName>(?<=repository: \\{\\{ \\.Values\\.nexus \\}\\}\/).+(?=.*))"
      ],
      "registryUrlTemplate": "docker.io"
    }
  ]
}
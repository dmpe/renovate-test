{
  "enabledManagers": [
    "regex"
  ],
  "regex": {
    "fileMatch": [
      "control_values.yaml.gotmpl"
    ]
  }, 
  "regexManagers": [
    {
      "fileMatch": [
        "control_values.yaml.gotmpl"
      ],
      "matchStringsStrategy": "recursive",
      "matchStrings": [
        "[^image:].+",
        "repository: (?<depName>.*)\n *tag: (?<currentValue>.*)"
      ],
      "datasourceTemplate": "docker"
    }
  ]
}
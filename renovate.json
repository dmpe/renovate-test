{
  "enabledManagers": [
    "regex"
  ],
  "regex": {
    "fileMatch": [
      "control_values.yaml.gotmpl"
    ]
  }, 
  "regexManagers": [
    {
      "fileMatch": [
        "control_values.yaml.gotmpl"
      ],
      "matchStringsStrategy": "recursive",
      "matchStrings": [
        "[^image:].+",
        "(?<depName>[^repository: \\{\\{ \\.Values\\.nexus \\}\\}\/].*)\n"
      ],
      "datasourceTemplate": "docker",
      "currentValueTemplate": "docker"
    }
  ]
}